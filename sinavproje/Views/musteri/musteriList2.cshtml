@model List<sinavproje.Models.dbmusteri>

@{
    ViewBag.Title = "musteriList2";
    Layout = "~/Views/Shared/_LayoutWebPages.cshtml";
}

<table>
    <tr>
        <th>musteri Adı:    </th>
        <th>tlefon:     </th>
        <th>email:    </th>
        <th>resim:    </th>
        <th>kayit tarih:    </th>
        <th>puna:      </th>
        <th>aciklama:   </th>
        <th>adres:     </th>
        <th>sehirid:   </th>
        <th>ilçeid:    </th>
     
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.musteriAdi   </td>
            <td>@item.telefon   </td>
            <td>@item.email   </td>
            <td>@item.resim   </td>
            <td>@item.kayitTarihi   </td>
            <td>@item.puan   </td>
            <td>@item.aciklama   </td>
            <td>@item.adres   </td>
            <td>@item.sehirId   </td>
            <td>@item.ilceId   </td>
     
        </tr>
    }
</table>

